#! /bin/sh
set -e

# source debconf library
. /usr/share/debconf/confmodule

db_settitle libmkl-rt/title

# potential license violation
db_input critical libmkl-rt/use-as-default-blas-lapack || true
db_go

# select the exact symlinks that should point to MKL
db_subst libmkl-rt/exact-so-3-selections DEB_HOST_MULTIARCH @DEB_HOST_MULTIARCH@
db_input high libmkl-rt/exact-so-3-selections || true
db_go

#DEBHELPER#

exit 0
